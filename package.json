{
	"name": "svelidate",
	"version": "2.6.1",
	"description": "Simple and lightweight form validation for Svelte with no dependencies",
	"author": "alexandre-fernandez",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/svelidate/svelidate"
	},
	"keywords": [
		"svelte",
		"sveltekit",
		"form",
		"validation",
		"reactive",
		"ssr",
		"progressive",
		"enhancement"
	],
	"main": "dist/svelidate.cjs.js",
	"types": "dist/index.d.ts",
	"files": [
		"/dist"
	],
	"scripts": {
		"build": "npm run lint && npm run test && tsc && vite build",
		"lint": "eslint --ignore-path .eslintignore .",
		"test": "vitest run",
		"coverage": "vitest run --coverage"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.29.0",
		"@typescript-eslint/parser": "^5.29.0",
		"c8": "^7.11.3",
		"eslint": "^8.18.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"i": "^0.3.7",
		"npm": "^8.13.1",
		"prettier": "^2.7.1",
		"typescript": "^4.5.4",
		"vite": "^2.9.9",
		"vite-plugin-dts": "^1.2.0",
		"vitest": "^0.16.0"
	}
}
